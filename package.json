    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1"
  }
}
